{
  "expo": {
    "name": "Chobegraphy",
    "slug": "chobegraphy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/dark-icon.jpg",
    "scheme": "chobegraphy",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.chobegraphy.app",
      "buildNumber": "1.0.0"
    },
    "android": {
      "package": "com.chobegraphy.app",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/dark-icon.jpg",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/dark-icon.jpg"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/light-splash.jpg",
          "imageDark": "./assets/images/dark-splash.jpg",
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "backgroundColorDark": "#000000"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": ""
      }
    },
    "owner": "chobegraphybd",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": ""
    }
  }
}
